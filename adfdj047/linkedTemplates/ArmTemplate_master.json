{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfdj047"
		},
		"ADLS_gen2DS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLS_gen2DS'"
		},
		"dpsqlDB_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'dpsqlDB'"
		},
		"ADLS_gen2DS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://azuredpadls.dfs.core.windows.net/"
		},
		"Rest_LS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://restcountries.com/v3.1/all"
		},
		"dpsqlDB_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "dj0047.database.windows.net"
		},
		"dpsqlDB_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "@{linkedService().databaseName}"
		},
		"dpsqlDB_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "dj0047"
		},
		"Storage_event_Trigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/a481d012-8e5a-41b3-98c8-994b250ecb29/resourceGroups/my_rg/providers/Microsoft.Storage/storageAccounts/azuredpadls"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfdj047_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADLS_gen2DS_accountKey": {
						"value": "[parameters('ADLS_gen2DS_accountKey')]"
					},
					"dpsqlDB_password": {
						"value": "[parameters('dpsqlDB_password')]"
					},
					"ADLS_gen2DS_properties_typeProperties_url": {
						"value": "[parameters('ADLS_gen2DS_properties_typeProperties_url')]"
					},
					"Rest_LS_properties_typeProperties_url": {
						"value": "[parameters('Rest_LS_properties_typeProperties_url')]"
					},
					"dpsqlDB_properties_typeProperties_server": {
						"value": "[parameters('dpsqlDB_properties_typeProperties_server')]"
					},
					"dpsqlDB_properties_typeProperties_database": {
						"value": "[parameters('dpsqlDB_properties_typeProperties_database')]"
					},
					"dpsqlDB_properties_typeProperties_userName": {
						"value": "[parameters('dpsqlDB_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adfdj047_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Storage_event_Trigger_properties_typeProperties_scope": {
						"value": "[parameters('Storage_event_Trigger_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adfdj047_ArmTemplate_0')]"
			]
		}
	]
}